@{
    ViewData["Title"] = "Home Page";
}
@{
    var tests = new[]
{
    new {Name="C#"},
    new {Name=".NET"},
    new {Name="JAVA"},
    new {Name="JavaScript"},
    new {Name="English"}
};

}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
</div>

@foreach(var test in tests){

                <div class="test">
                    <img class="test-img" src="" height="250" width="250"/>
                    <p class="test-title">@test.Name</p>
                </div>



}
